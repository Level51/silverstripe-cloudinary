{
  "name": "directlease/silverstripe-cloudinary",
  "version": "2",
  "time": "2020-12-16",
  "description": "Cloudinary image handling & upload field for SilverStripe",
  "keywords": [
    "silverstripe",
    "cloudinary"
  ],
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Kliemsch",
      "email": "dk@lvl51.de"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "silverstripe/framework": "^5",
    "cloudinary/cloudinary_php": "^2",
    "nesbot/carbon": "^2"
  },
  "extra": {
    "installer-name": "cloudinary",
    "expose": [
      "client"
    ]
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Level51\\Cloudinary\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
